@model IEnumerable<Lab2.Models.Grade>

@{
    ViewBag.Title = "Kurso pažymiai";
}

<h2>Kurso pažymiai</h2>

<table class="table">
    <tr>
        <th>
            Projektas
        </th>
        <th>
            Pažimys
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mark)
        </td>

    </tr>
}

</table>
